{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended"
    ],
    "jsRules": {},
    "rules": {
        "no-duplicate-variable": true,
        "object-literal-sort-keys": false,
        "trailing-comma": false,
        "no-console": false,
        "member-access": false,
        "no-empty": false,
        "prefer-const": false,
        "no-shadowed-variable": false,
        "no-consecutive-blank-lines": false,
        "quotemark": [
            true,
            "single"
        ],
        "arrow-parens": [
            false,
            "ban-single-arg-parens"
        ]
    },
    "rulesDirectory": []
}